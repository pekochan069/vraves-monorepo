{
	"compilerOptions": {
		"target": "es2021",
		"lib": ["es2021"],
		"module": "nodeNext",
		"moduleResolution": "nodeNext",
		"types": ["@cloudflare/workers-types"],
		"allowJs": true,
		"checkJs": false,
		"noEmit": true,
		"isolatedModules": true,
		"allowSyntheticDefaultImports": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true
	}
}
